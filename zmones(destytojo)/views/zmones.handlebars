<a href="/zmones/naujas">Naujas</a>
<table>
{{#each zmones}}
    <tr>
        <td>
            {{this.vardas}}
        </td>
        <td>
            {{this.pavarde}}
        </td>
        <td>
            {{dateFormat this.gimimoData}}
        </td>
        <td>
            {{this.alga}}
        </td>
        <td>
            <a href="/zmones/{{this.id}}">edit</a>
        </td>
        <td>
            <a href="/zmones/{{this.id}}/delete">X</a>
        </td>
        <td>
            <a href="/zmones/{{this.id}}/kontaktai">Kontaktai</a>
        </td>
    </tr>
{{/each}}
</table>
